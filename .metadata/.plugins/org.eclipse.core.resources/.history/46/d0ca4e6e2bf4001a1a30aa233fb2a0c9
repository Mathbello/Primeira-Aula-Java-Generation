package ExerciciosVetor;

import java.util.Scanner;

public class Exercicio01 {
	public static void main(String args[]) {
		Scanner scan= new Scanner(System.in);
		int[] A= {1, 0, 5, -2, -5, 7};
		int soma= 0;
		
		for(int i= 0; i<6; i++) {
			if(A[i]==0) {
				soma+= A[i];
				System.out.println(soma);
			}
			if(A[i]==1) {
				soma+= A[i];
				System.out.println(soma);
			}
			if(A[i]==5) {
				soma+= A[i];
				System.out.println(soma);
			}
			
		}
		
		//System.out.println(soma);
		
	}

}
