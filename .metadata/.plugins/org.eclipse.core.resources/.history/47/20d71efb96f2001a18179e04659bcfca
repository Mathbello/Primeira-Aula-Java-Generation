/*
 * Faça um programa que receba três inteiros e diga qual deles é o maior.
 */

package ExerciciosDecisao;

import java.util.Scanner;

public class Decisao01 {
	public static void main(String args[]) {
		int n1, n2, n3;
		Scanner scan= new Scanner(System.in);
		
		System.out.printf("Insira o valor do número 1: ");
		n1= scan.nextInt();
		System.out.printf("Insira o valor do número 2: ");
		n2= scan.nextInt();
		System.out.printf("Insira o valor do número 3: ");
		n3= scan.nextInt();
	}

}
